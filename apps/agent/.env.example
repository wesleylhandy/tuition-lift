# Database — Postgres connection for LangGraph checkpointer (003)
DATABASE_URL=postgresql://user:password@host:5432/database

# Tavily — web search for Advisor discovery (003)
TAVILY_API_KEY=

# LLM — query generation for discovery (004)
OPENAI_API_KEY=

# Optional: rate limit between search batches (FR-014; default 2000 ms)
# Minimum delay in ms between Tavily calls to prevent throttling. — 004
DISCOVERY_SEARCH_BATCH_DELAY_MS=2000

# Optional: timeout for Tavily search batch (default 300000 ms = 5 min; T032)
# Prevents indefinite hang if Tavily API is slow or unresponsive.
DISCOVERY_SEARCH_TIMEOUT_MS=300000

# College Scorecard — institution catalog and COA for ROI comparison (009)
COLLEGE_SCORECARD_API_KEY=
# BLS — career outcomes (year-5 income) for ROI comparison (009)
BLS_API_KEY=

# Scout — manual scholarship ingestion (007)
# Optional: max file size in MB (default 10)
# SCOUT_MAX_FILE_SIZE_MB=10
# Optional: fuzzy dedup similarity 0–1 (default 0.85)
# SCOUT_DEDUP_SIMILARITY_THRESHOLD=0.85
# Optional: vision model for image/PDF extraction (default gpt-4o)
# SCOUT_VISION_MODEL=gpt-4o

# LangSmith — tracing for LangGraph (003 FR-019)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=
