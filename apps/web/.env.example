# Supabase (002-db-core-infrastructure)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Database — Postgres connection for LangGraph checkpointer (003)
DATABASE_URL=postgresql://user:password@host:5432/database

# Inngest — event ingestion and cron (003)
INNGEST_SIGNING_KEY=
INNGEST_EVENT_KEY=

# Prioritization refresh cron (003 US4); unix-cron format, e.g. "0 6 * * *" = 6 AM UTC daily
PRIORITIZATION_CRON_SCHEDULE=0 6 * * *

# Coach — email delivery and cron schedules (005)
RESEND_API_KEY=
# From address for Coach emails (Resend verified domain)
COACH_FROM_EMAIL=TuitionLift <notifications@tuitionlift.com>
# Game plan batch; runs after 003 prioritization at 06:00 UTC (plan.md: 005 at 06:30)
COACH_GAME_PLAN_CRON=30 6 * * *
# Deadline urgency check; hourly to meet SC-004 (within 2h of threshold)
COACH_DEADLINE_CHECK_CRON=0 * * * *
# Micro-task staleness check; every 4h to meet SC-007 (within 4h of 48h threshold)
COACH_MICRO_TASK_CHECK_CRON=0 */4 * * *
# Check-in batch backfill; daily for apps submitted 21+ days ago (FR-011)
COACH_CHECK_IN_BATCH_CRON=0 7 * * *

# Tavily — web search for Advisor discovery (003)
TAVILY_API_KEY=

# LangSmith — tracing for LangGraph (003 FR-019)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=

# Encryption — SAI at rest (003 FR-014); must decode to 32 bytes (256 bits)
# Generate: openssl rand -base64 32
ENCRYPTION_KEY=
