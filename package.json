{
  "name": "tuition-lift",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:local": "concurrently -k -n web,inngest \"pnpm --filter web dev\" \"pnpm dlx inngest-cli@latest dev -u http://localhost:3000/api/inngest\"",
    "dev:local:full": "pnpm --filter @repo/db exec supabase start && pnpm dev:local",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "verify-checkpoints": "tsx apps/agent/scripts/verify-checkpoints.ts",
    "verify-sc001": "tsx apps/agent/scripts/verify-sc001.ts",
    "verify-us1": "tsx apps/agent/scripts/verify-us1.ts",
    "verify-us2": "tsx apps/agent/scripts/verify-us2.ts",
    "verify-us3": "tsx apps/agent/scripts/verify-us3.ts",
    "verify-us4": "tsx apps/agent/scripts/verify-us4.ts",
    "setup-test-profile": "tsx apps/agent/scripts/setup-test-profile.ts"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "prettier": "^3.7.4",
    "tsx": "^4.19.2",
    "turbo": "^2.8.4",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
